  "scoring": {
    "weights": {
      "email_quality": 0.05,
      "company_relevance": 0.50,
      "geographic_priority": 0.25,
      "engagement": 0.20
    },

    "thresholds": {
      "high_priority": 120,
      "medium_priority": 60,
      "low_priority": 20,
      "exclude": 0
    },

    "bonus_multipliers": {
      "oem_manufacturer": 1.5,
      "czech_company": 1.3,
      "target_geography": 2.0,
      "technical_domain": 1.2,
      "distributor": 1.4,
      "multi_brand": 1.3
    }
  },

  "exclusion_rules": {
    "hard_exclusions": [
      "Free personal email domain (seznam.cz, gmail.com, etc.)",
      "HR/recruitment prefix (hr@, jobs@, kariera@)",
      "System/noreply email addresses",
      "Search engine or directory domain",
      "Government/public sector",
      "Healthcare/medical",
      "Education institution",
      "Finance/insurance/legal services"
    ],
    "soft_exclusions": [
      "Construction services without equipment keywords",
      "Rental-only without sales indicators",
      "General e-commerce without equipment focus",
      "Media/advertising companies"
    ],
    "exclusion_logic": "If ANY hard_exclusion matches → score = 0 (EXCLUDED). Soft exclusions apply score penalties."
  },

  "notes": {
    "purpose": "Aggressive filtering for Czech hydraulic construction equipment market. Goal: Exclude 75-85% of contacts, keeping only genuine OEM manufacturers, distributors, and dealers.",
    "target_output": "HIGH: 2-3% (60-90 contacts), MEDIUM: 5-8% (150-230), LOW: 10-15% (290-435), EXCLUDED: 75-85% (2000+)",
    "optimization": "Emphasizes company_relevance (50%) and negative keywords to maximize exclusion while preserving top leads."
  }
