    "equipment_sales": {
      "primary": [
        "prodej", "sales", "obchod", "commerce",
        "dodávka", "supply", "distribuce", "distribution"
      ],
      "secondary": [
        "nové stroje", "new equipment",
        "použité stroje", "used equipment",
        "zánovní", "like new"
      ],
      "score": 70
    },

    "components_parts": {
      "primary": [
        "náhradní díly", "spare parts", "parts",
        "součástky", "components", "příslušenství", "accessories"
